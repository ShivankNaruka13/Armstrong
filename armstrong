#include <stdio.h>

void main()
{
    int num;
    scanf("%d",&num);
    int rem;
    int temp=num;
    
    for(int i=0;num>0;i++){
        temp=num%10;
        rem+=temp*temp*temp;
        num=num/10;
    }
    if(rem==num)
    printf("Number is Armstrong....");
    else
    printf("Number is not Armstrong....");
    
}
